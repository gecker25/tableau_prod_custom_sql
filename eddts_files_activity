SELECT "FILE_ACTIVITY"."ACTION" AS "ACTION",
  "FILE_ACTIVITY"."AT" AS "AT",
  "FILE_ACTIVITY"."DESTINATION" AS "DESTINATION",
  "FILE_ACTIVITY"."FILE_NAME" AS "FILE_NAME",
  "FILE_ACTIVITY"."FOLDER" AS "FOLDER",
  "FILE_ACTIVITY"."FOLDER_LENGTH" AS "FOLDER_LENGTH",
  "FILE_ACTIVITY"."INTERFACE" AS "INTERFACE",
  "FILE_ACTIVITY"."IP" AS "IP",
  "FILE_ACTIVITY"."PATH" AS "PATH",
  "FILE_ACTIVITY"."SIZE" AS "SIZE",
  "FILE_ACTIVITY"."TYPE" AS "TYPE",
  "FILE_ACTIVITY"."USERNAME" AS "USERNAME"
FROM "ANALYTICS"."MARTS"."FILE_ACTIVITY" "FILE_ACTIVITY"
WHERE   "FILE_ACTIVITY"."FILE_NAME" IS NOT NULL
